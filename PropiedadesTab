                foreach (var ContP in Model.ConteoPropiedades)
                {
                    if (ContP.EstatusPropiedad == "En revisión")
                    {
                        NoP = ContP.Conteo;

                    }
                    if (ContP.EstatusPropiedad == "Aprobada")
                    {

                        PuP = ContP.Conteo;
